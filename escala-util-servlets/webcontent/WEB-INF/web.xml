<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Escala Utility Services</display-name>
  <servlet>
    <servlet-name>file-parser</servlet-name>
    <servlet-class>com.escala.util.FileParserServlet</servlet-class>
    <init-param>
      <param-name>max-file-size</param-name>
      <param-value>1000000</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>file-parser</servlet-name>
    <url-pattern>/FileParser</url-pattern>
  </servlet-mapping>
</web-app>